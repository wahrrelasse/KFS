<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.kfs.db.controller.DeleteViewPresenter">
   <Text fill="#8d0909" strokeType="OUTSIDE" strokeWidth="0.0" text="Löschen:" underline="true">
      <font>
         <Font size="25.0"/>
      </font>
   </Text>
   <HBox alignment="CENTER" prefHeight="188.0" prefWidth="300.0" spacing="20.0">
      <padding>
         <Insets bottom="30.0"/>
      </padding>
      <TextField fx:id="deleteNumberField" promptText="Nummer..."/>
      <Button fx:id="confirmDeleteButton" mnemonicParsing="false" onAction="#onDeleteButtonPressed" prefHeight="35.0"
              prefWidth="100.0" text="Löschen" textFill="#9a0303"/>
   </HBox>
</VBox>
