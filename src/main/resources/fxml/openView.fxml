<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.kfs.db.controller.OpenViewPresenter">
    <MenuBar VBox.vgrow="NEVER">
        <Menu mnemonicParsing="false" text="File">
            <MenuItem fx:id="openNew" mnemonicParsing="false" onAction="#onOpenNewPressed" text="Neu"/>
            <MenuItem fx:id="openMenu" mnemonicParsing="false" onAction="#onOpenBikePressed" text="Öffne Räder"/>
            <MenuItem fx:id="openEMenu" mnemonicParsing="false" onAction="#onOpenEBikesPressed" text="Öffne E-Bikes"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem mnemonicParsing="false" text="Anleitung"/>
        </Menu>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
        <ImageView fx:id="logoSpace" fitHeight="375.0" fitWidth="640.0" layoutY="-1.0" pickOnBounds="true"
                   preserveRatio="true"/>
    </AnchorPane>
</VBox>
